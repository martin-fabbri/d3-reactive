    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/d3-dsv": "1.0.31",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-transition-group": "^2.0.9",
    "autoprefixer": "^8.5.0",
    "postcss-cli": "^5.0.0",
    "tailwindcss": "^0.5.3",
    "typescript": "^2.8.3"
  }
}
