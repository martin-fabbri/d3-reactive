{
  "name": "d3-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^2.3.1",
    "d3": "^5.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts-ts": "2.16.0",
    "react-transition-group": "^2.3.1",
    "styled-components": "^3.3.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
